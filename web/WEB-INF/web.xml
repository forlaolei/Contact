<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>DealLogin</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.DealLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListContact</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.ListContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddContact</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.AddContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteContact</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.DeleteContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryContact</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.QueryContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateContact</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.UpdateContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>cn.laolei.contact.servlet.Register</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DealLogin</servlet-name>
        <url-pattern>/DealLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListContact</servlet-name>
        <url-pattern>/ListContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddContact</servlet-name>
        <url-pattern>/AddContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteContact</servlet-name>
        <url-pattern>/DeleteContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryContact</servlet-name>
        <url-pattern>/QueryContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateContact</servlet-name>
        <url-pattern>/UpdateContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
</web-app>